{
  "name": "carbon-tutorial-web-components",
  "homepage": "https://github.com/carbon-design-system/carbon-tutorial-web-components",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "preview": "vite preview",
    "lint:es": "eslint . main.js --report-unused-disable-directives --max-warnings 0",
    "lint:format": "prettier ./**/*.{js,jsx,ts,tsx,md,mdx,scss} --write --ignore-unknown --no-error-on-unmatched-pattern --log-level warn",
    "lint:spell": "cspell lint --quiet \"**/*.{js,css,scss,md}\"",
    "lint:style": "pnpm stylelint '**/*.scss'"
  },
  "devDependencies": {
    "@double-great/stylelint-a11y": "^3.0.2",
    "cspell": "^8.15.5",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "gh-pages": "^6.2.0",
    "globals": "^15.11.0",
    "prettier": "^3.3.3",
    "stylelint": "^16.10.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-plugin-carbon-tokens": "^3.2.1",
    "stylelint-use-logical-spec": "^5.0.1",
    "vite": "^5.4.0"
  },
  "dependencies": {
    "@carbon/icons": "^11.52.0",
    "@carbon/styles": "^1.69.0",
    "@carbon/web-components": "^2.17.0",
    "@octokit/core": "^6.1.2",
    "sass": "^1.80.4"
  }
}
